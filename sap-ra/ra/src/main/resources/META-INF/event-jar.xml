<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN" "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">
<event-jar>
    <description>The event definitions for SAP RA.</description>
    <library-ref>
        <description> JAIN SIP SDP </description>
        <library-name> JAIN SIP SDP </library-name>
        <library-vendor> javax.sdp </library-vendor>
        <library-version> 1.1 </library-version>
    </library-ref>

    <event-definition>
        <description>Fired when an announcement is updated or created.</description>
        <event-type-name> sap.event.AnnouncementEvent.ANNOUNCEMENT </event-type-name>
        <event-type-vendor> ${pom.groupId} </event-type-vendor>
        <event-type-version> ${pom.version} </event-type-version>
        <event-class-name>sap.event.AnnouncementEvent</event-class-name>
    </event-definition>

    <event-definition>
        <description>Fired when an announcement is deleted.</description>
        <event-type-name> sap.event.AnnouncementEvent.DELETE_ANNOUNCEMENT </event-type-name>
        <event-type-vendor> ${pom.groupId} </event-type-vendor>
        <event-type-version> ${pom.version} </event-type-version>
        <event-class-name>sap.event.AnnouncementEvent</event-class-name>
    </event-definition>
</event-jar>