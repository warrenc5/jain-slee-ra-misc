<resource-adaptor-type-jar>
    <resource-adaptor-type id="xmlrpc">
        <description> XmlRpc Resource Adaptor Type </description>
        <resource-adaptor-type-name> XmlRpc Resource Adaptor Type </resource-adaptor-type-name>
        <resource-adaptor-type-vendor> MOFOKOM </resource-adaptor-type-vendor>
        <resource-adaptor-type-version> ${pom.version} </resource-adaptor-type-version>

        <library-ref>
