<?xml version="1.0"?>
<!DOCTYPE oc-sbb-jar PUBLIC "-//Open Cloud Ltd.//DTD JAIN SLEE SBB Extension 1.0//EN" "http://www.opencloud.com/slee/oc-sbb-jar_1_0_1.dtd">
<oc-sbb-jar>
    <description></description>
    <sbb id="JPASBB">
        <resource-ref>
            <res-ref-name>MyDataSource</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
            <res-jndi-name>jdbc/JPA-JDBC-Datasource</res-jndi-name>
        </resource-ref>
        <resource-ref>
            <res-ref-name>MyUnmanagedDataSource</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
            <res-jndi-name>jdbc/JPA-JDBC-Unmanaged-Datasource</res-jndi-name>
        </resource-ref>
    </sbb>
</oc-sbb-jar>